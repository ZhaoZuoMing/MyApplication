<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

     <TextView
         android:id="@+id/kefu_time_txt"
         android:layout_width="match_parent"
         android:layout_height="wrap_content"
         android:text="2017-02-27 11:22:18"
         android:textSize="13dp"
         android:gravity="center_horizontal"
         android:layout_marginTop="@dimen/font_size_10"
         />
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <RelativeLayout
            android:layout_width="40dp"
            android:layout_height="38dp"
            android:background="@drawable/header_kefu"
            android:id="@+id/kefu_icon_img"
            android:layout_alignTop="@+id/k_f_content_lay"
            android:layout_toRightOf="@+id/k_f_content_lay"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="@dimen/font_size_10"
            android:layout_alignParentStart="true" >

            <TextView
                android:text="客服"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                android:textColor="@color/colorPrimary"
                android:id="@+id/textView18" />
        </RelativeLayout>
        <LinearLayout
            android:id="@+id/k_f_content_lay"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:background="@drawable/shape_rectangle_white"
            android:layout_marginLeft="60dp"
            android:layout_marginRight="50dp"
            android:layout_marginTop="5dp"
            android:orientation="vertical"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentTop="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true">
            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/gray_color"
                android:layout_margin="@dimen/font_size_10"
                android:text="@string/kefu_txt"
                />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_margin="3dp"
                android:background="@color/cadender_bg_color"
                android:orientation="vertical"
                >
                <RelativeLayout
                    android:id="@+id/airline_train_hotel_order_lay"
                    android:layout_width="match_parent"
                    android:layout_height="35dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimary"
                        android:text="@string/kefu_piao_txt"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/font_size_10"
                        />

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@mipmap/arrow_right"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:layout_marginEnd="12dp"
                        android:id="@+id/image01" />
                </RelativeLayout>
              <TextView
                  android:layout_width="match_parent"
                  android:layout_height="@dimen/line_height"
                  android:background="@color/light_gray"
                  />
                <RelativeLayout
                    android:id="@+id/qian_zhen_baoxian_lay"
                    android:layout_width="match_parent"
                    android:layout_height="35dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimary"
                        android:text="@string/qianzheng_baoxian_txt"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/font_size_10"
                        />

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@mipmap/arrow_right"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:layout_marginEnd="12dp"
                        android:id="@+id/image02" />
                </RelativeLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="@color/light_gray"
                    />
                <RelativeLayout
                    android:id="@+id/dingzhi_lay"
                    android:layout_width="match_parent"
                    android:layout_height="35dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimary"
                        android:text="@string/dingzhi_txt"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/font_size_10"
                        />

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@mipmap/arrow_right"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:layout_marginEnd="12dp"
                        android:id="@+id/image03" />
                </RelativeLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="@color/light_gray"
                    />
                <RelativeLayout
                    android:id="@+id/huifu_lay"
                    android:layout_width="match_parent"
                    android:layout_height="35dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimary"
                        android:text="@string/huizhan_txt"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/font_size_10"
                        />

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@mipmap/arrow_right"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:layout_marginEnd="12dp"
                        android:id="@+id/image04" />
                </RelativeLayout>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/line_height"
                    android:background="@color/light_gray"
                    />
                <RelativeLayout
                    android:id="@+id/jiesonvip_lay"
                    android:layout_width="match_parent"
                    android:layout_height="35dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="@color/colorPrimary"
                        android:text="@string/jieson_vip"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="@dimen/font_size_10"
                        />

                    <ImageView
                        android:layout_width="15dp"
                        android:layout_height="15dp"
                        android:src="@mipmap/arrow_right"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentEnd="true"
                        android:layout_centerVertical="true"
                        android:layout_marginRight="12dp"
                        android:layout_marginEnd="12dp"
                        android:id="@+id/imageView12" />
                </RelativeLayout>

            </LinearLayout>

        </LinearLayout>
    </RelativeLayout>

</LinearLayout>
